{
	"name": "fock-logger",
	"version": "0.2.3-a",
	"description": "Simple logger for your pet-project",
	"main": "index.js",
	"scripts": {
		"build:file": "tsc && tsc --declaration",
		"build": "pnpm run build:file && node ../dist/build.js",
		"watch:build": "nodemon build.ts",
		"format": "prettier ./**.ts --write",
		"test": "ts-mocha --paths ./test/**/*.test.ts"
	},
	"files": [
		"loggers.types.d.ts",
		"loggers.types.js",
		"loggers.names.d.ts",
		"loggers.names.js",
		"file.logger.d.ts",
		"file.logger.js",
		"index.logger.d.ts",
		"index.logger.js",
		"index.d.ts",
		"index.js"
	],
	"keywords": [
		"logger",
		"simple",
		"simple logger"
	],
	"author": "FOCKUSTY",
	"license": "MIT",
	"devDependencies": {
		"@types/expect": "^24.3.0",
		"@types/mocha": "^10.0.8",
		"@types/node": "^22.7.4",
		"mocha": "^10.7.3",
		"nodemon": "^3.1.7",
		"prettier": "^3.3.3",
		"ts-mocha": "^10.0.0",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.6.2"
	},
	"engines": {
		"node": ">=7.6.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/FOCKUSTY/f-server"
	},
	"bugs": {
		"email": "wwddwwwd@mail.ru",
		"url": "https://github.com/FOCKUSTY/f-server/issues"
	},
	"dependencies": {
		"f-formatter": "^0.2.9",
		"node": "^22.9.0"
	}
}
